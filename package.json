{
  "name": "guidebook.github.io",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@callstack/react-theme-provider": "3.0.3",
    "@evilfactory/global-state": "^0.0.1",
    "atomize": "^1.0.4",
    "babel-jest": "24.7.1",
    "formik": "^1.5.8",
    "react": "^16.9.0-rc.0",
    "react-dom": "^16.9.0-rc.0",
    "react-scripts": "^3.0.0-next.68",
    "react-transition-group": "4.2.2",
    "styled-components": "4.3.2",
    "yup": "0.27.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "cross-env CI=true react-scripts test --env=jsdom --coverage --verbose",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "globals": {
      "render": true,
      "cleanup": true,
      "fireEvent": true,
      "waitForElement": true,
      "wait": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "4.0.0",
    "@testing-library/react": "8.0.7",
    "@testing-library/react-hooks": "1.1.0",
    "cross-env": "5.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/src/serviceWorker.js",
      "!<rootDir>/node_modules/"
    ]
  }
}
